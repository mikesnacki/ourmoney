enum LineItemType {
  INCOME
  EXPENSE
}

type LineItem {
  id: ID! @id
  createdAt: DateTime! @createdAt
  name: String!
  amount: Float!
  type: LineItemType!
  user: User!
}

type User {
  id: ID! @id
  name: String!
  email: String!
  relatedUser: User @relation(link: INLINE)
  lineItems: [LineItem]
}
