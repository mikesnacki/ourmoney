enum LineItemType {
  INCOME
  EXPENSE
}

type LineItem {
  id: ID! @id
  createdAt: DateTime! @createdAt
  name: String!
  amount: Float!
  type: String!
  deleted: Boolean
  user: User!
}

type User {
  id: ID! @id
  email: String! @unique
  relatedUser: User @relation(link: INLINE)
  lineItems: [LineItem]
}
