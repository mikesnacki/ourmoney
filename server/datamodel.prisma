type User {
  id: ID! @id @unique
  name: String!
  email: String! @unique
  createdAt: DateTime! @createdAt
  incomes: [Income] @relation(link: INLINE)
  expenses: [Expense] @relation(link: INLINE)
}

type Income {
  id: ID! @id
  createdAt: DateTime! @createdAt
  title: String!
  amount: Float
  user: User! 
}

type Expense {
  id: ID! @id
  createdAt: DateTime! @createdAt
  title: String!
  amount: Float
  user: User! 
}